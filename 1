#include<stdio.h>
void input_process_detail();
int main(){
  	int ch,a=0;
  	while(a==0){
  	printf("\n1.process sheduling\n2.exit");
  	printf("\n\n\nselect your choice :");
  	scanf("%d",&ch);
  	switch(ch){
  		case 1:
  			input_process_detail();
  			 break;
  		case 2:
  			a=1;
  			break;
  		default:
  			printf("Wrong choice");
	  }
	}
  return 0;
}
